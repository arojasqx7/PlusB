
@{
    ViewBag.Title = "anweredSurvey";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
    <section class="content-header">
        <h1>Survey Satisfaction</h1>
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="error-page">
            <h2 class="headline text-primary"> <span class="glyphicon glyphicon-share-alt"></span></h2>
            <br />
            <div class="error-content">
                <h3><i class="fa fa-warning text-primary"></i> Survey already answered.</h3>
                <p> We could not find the survey you were looking for. Probably already is filled.
                    Meanwhile, you may <a href="@Url.Action("Login","Account")">login</a> to continue the work flow.
                </p>
            </div>
        </div>
     </section>